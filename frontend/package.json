  "scripts": {
    "setup:lint": "npm install --save-dev eslint@8.57.0 prettier@3.3.3 @typescript-eslint/parser@5.62.0 @typescript-eslint/eslint-plugin@5.62.0 eslint-config-prettier@9.1.0 eslint-plugin-prettier@5.2.1",
